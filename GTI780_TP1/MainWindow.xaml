<Window x:Name="MainWindow1" x:Class="GTI780_TP1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="" WindowStyle="None" ShowInTaskbar="False" ResizeMode="NoResize"
        WindowStartupLocation="Manual" AllowsTransparency="True"
        Margin="0" Left="0" Top="0">
    <Grid x:Name="Grid1" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Margin="0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Image x:Name="PictureBox1"
               Source="{Binding ImageSource1}"
               Stretch="UniformToFill" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0"/>
        <Image x:Name="PictureBox2"
               Source="{Binding ImageSource2}"
                Stretch="None" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Image Name="HeaderImage" Source="EnTeteIncorrect.bmp"
               Stretch="None" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Top" />
    </Grid>
</Window>
